    </xs:simpleType>
    <xs:simpleType name="returnDueDateType">
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="autoTreatmentSuppressionStartDateType">
        <xs:annotation>
