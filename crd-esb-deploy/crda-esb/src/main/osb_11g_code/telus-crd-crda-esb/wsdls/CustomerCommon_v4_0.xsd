            <xs:documentation>Specific comments about restrictions on
                customer contact.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="preferredContactMethodCodeType">
        <xs:annotation>
            <xs:documentation>e.g. Work Phone, Cell Phone, Home Phone,
                Regular Mail, No Contact Desired, Restricted Contact,
                SMS : Notification via SMS, Web :  Portal Web Portal,
                Face-to-face :  meeting Face-to-face meeting, EMAIL  :
                Notification via email, Restricted contact.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="preferredContactMethodCommentType">
