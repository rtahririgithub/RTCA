            <xs:documentation>The date at which the bill redirect expires.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="billRedirectOperatorIdType">
        <xs:annotation>
            <xs:documentation>The id of the operator the bill document
                will be redirected to.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="billRedirectOperatorNameType">
        <xs:annotation>
            <xs:documentation>The name of the operator the bill document
                will be redirected to.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="billRedirectReasonCodeType">
