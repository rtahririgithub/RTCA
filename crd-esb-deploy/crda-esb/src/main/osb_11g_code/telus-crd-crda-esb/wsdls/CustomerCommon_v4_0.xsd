<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
    targetNamespace="http://xmlschema.tmi.telus.com/xsd/Customer/BaseTypes/CustomerCommon_v4"
    version="4.0"
    xmlns:tns="http://xmlschema.tmi.telus.com/xsd/Customer/BaseTypes/CustomerCommon_v4" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <!-- Simple Common Element Types -->
    <xs:simpleType name="entityIdType">
        <xs:annotation>
            <xs:documentation>Id for an Entiry Type.  Examples:
                CUSTOMER, SRVC_INST, etc.  RefODS- BI_CM_ENTITY_TYPE</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:long"/>
    </xs:simpleType>
    <xs:simpleType name="customerIdType">
        <xs:annotation>
            <xs:documentation>Unique meaningless identifier for a
                customer.  Id from Customer ODS and Customer Profile</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:long"/>
    </xs:simpleType>
    <xs:simpleType name="billingAccountIdType">
        <xs:annotation>
            <xs:documentation>Unique meaningless identifier for a
                billing account.  Id is from Customer ODS and Customer Profile.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:long"/>
    </xs:simpleType>
    <xs:simpleType name="billingAccountNumberType">
        <xs:annotation>
            <xs:documentation>Business Key ID for looking up a Billing
                Account (this is the number that is on a cusotmers
                bill).  Requires a source mastering system (master
                source id) to uniquely identify a billing account</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="payChannelIdType">
        <xs:annotation>
            <xs:documentation>Unique meaingless identifier for a pay
                channel.  Id is from Customer ODS and Customer Profile</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int"/>
    </xs:simpleType>
    <xs:simpleType name="payChannelNumberType">
        <xs:annotation>
            <xs:documentation>Business pay channel number.  Business
                identifier for the pay channel and is related to a
                source billin system.  It must be used in conjunction
                with a billing system id to be unique</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="billMediaIdType">
        <xs:annotation>
            <xs:documentation>Unique meaningless identifier for bill media.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int"/>
    </xs:simpleType>
    <xs:simpleType name="addressIdType">
        <xs:annotation>
            <xs:documentation>Unique meaningless identifier for address</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:long"/>
    </xs:simpleType>
    <xs:simpleType name="addressTypeCodeType">
        <xs:annotation>
            <xs:documentation>Type of Address.  Examples: C- Civic, G-
                General Delivery, etc. RefPDS: BI_CM_ADDRESS_TYPE</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="nameIdType">
        <xs:annotation>
            <xs:documentation>Unique Identifier for name.  </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:long"/>
    </xs:simpleType>
    <xs:simpleType name="nameTypeCodeType">
        <xs:annotation>
            <xs:documentation>Type of party that the name is for.
                Examples: P- Person, O- Orgaization, U- Unknown</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="civicNumberType">
        <xs:annotation>
            <xs:documentation>The official number assigned to an address
                by the municipality.  EXAMPLES: 2514</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="civicNumberSuffixType">
        <xs:annotation>
            <xs:documentation>Typically an alpha or fractional suffix to
                a civic number e.g. &quot;A&quot; or &quot;1/2&quot;
                appended after the civic number.EXAMPLES:B, 2 (for 1/2), etc.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="ruralRouteNumberType">
        <xs:annotation>
            <xs:documentation>The number used to identify a rural route.
                Must be combined with the rural route code to create a
                valid address element. EXAMPLES: &quot;8&quot;</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="ruralRouteTypeCodeType">
        <xs:annotation>
            <xs:documentation>A code used to represent a Rural Route
                designator. EXAMPLES: RR - Rural Route, SS - Suburban
                Service, MR - Mobile Route, GD - General Delivery.
                RefPDS; RURAL_ROUTE_TYP_CUSTPF</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="stationAreaNameType">
        <xs:annotation>
            <xs:documentation>The name of the village, town, city, or
                metropolitan area which forms part of the Canada Post
                station name. EXAMPLES: MISSION, MEDICINE HAT, SURREY,
                WINNIPEG.  COMMENTS: Must be combined with the station
                type code to create a valid address element.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="stationQualifierType">
        <xs:annotation>
            <xs:documentation>The name used to identify a Canada Post
                Station Qualifier.  Must be used in conjuction with a
                Station Area Name. EXAMPLES: A, MAIN</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="stationTypeCodeType">
        <xs:annotation>
            <xs:documentation>The abbreviation used to identify a type
                of Canada Post Station. EXAMPLES: STN - Station, RPO -
                Retail Postal Outlet.  RefPDS- STATION_TYPE_CA_POST</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="additionalAddressInformationType">
        <xs:annotation>
            <xs:documentation>Optional data that a mailer wishes to
                include or Non-fielded address information. Typically
                used for USA or International Address. Types that are
                not validated more specifically than State and Country
                respectively.  EXAMPLES: SITE/ COMPARTMENT 3,
                LOT/CONCESSION 45.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="renderedAddressType">
        <xs:annotation>
            <xs:documentation>The first line of the rendered address.
                EXAMPLES: 201 - 2514B Acacia Dr</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="addrAssgnmtIdType">
        <xs:annotation>
            <xs:documentation>Automatically generated number to uniquely
                identify the Address Assignment.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:long"/>
    </xs:simpleType>
    <xs:simpleType name="addressAssignmentSubTypeCodeType">
        <xs:annotation>
            <xs:documentation>Identifies the type of the entity to
                address relationship denoting the use of the given
                address (As specified by the addr_id
                attribute).EXAMPLES: M: Mailing Billing Address (Billing
                Account) T: Billing Tax Address (Billing Account) E:
                Billing Email Address (Billing Account) N - Primary
                Contact Address (Contact Indivdual) C - Customer
                Official Address (Customer) S - Service Address (Service
                Instance) D - Default Billing Address (Customer) : Used
                for populating a default addresses to the user interface
                for CSRs ease of use. This could be changed by CSRs.
                Introduced as part of SDSE. RefPDS: ADDR_ASGNMT_TYPE</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="addressAssignmentTypeCodeType">
        <xs:annotation>
            <xs:documentation>Further classification of the address
                assignment type, e.g. original, standardized. EXAMPLES:
                O - original (default) C - cleansed  (output from
                Trillium) P - parsed (output from Trillium)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="addressMatchingStatusCodeType">
        <xs:annotation>
            <xs:documentation>Description: A code signifying an address
                validation status.  Example: V; I; C; N.  V- Verifiable,
                C- Correctable, N- Not correctable</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="addressSearchTextType">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="canadaPostBuildNameType">
        <xs:annotation>
            <xs:documentation>Name of building as obtained from canada
                post directories. TD Tower, Telus building  etc..</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="canadaPostLocnNameType">
        <xs:annotation>
            <xs:documentation>The name of the organization at the
                address. TELUS Communications ( for 3777 kingsway) .
                This value is taken from Canada post directories and
                filled with the trillium output for business customers.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="canadaPostRecordTypeType">
        <xs:annotation>
            <xs:documentation>The record type(classification)
                information as given by trillium based on canada post
                directory. Note : This has no direct relation or meaning
                as the 'address type'. This field may be useful in
                determining the type of the record in telus.  This field
                is available from the Canada Post Directories and define
                the address type for a given address or addresses.  For
                example, for 3777 Kingsway, the address type is ¿B¿ for
                Large Volume Receiver.  </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="careOfType">
        <xs:annotation>
            <xs:documentation>Optional data that a mailer wishes to
                include indicating the identity of a party able to take
                temporary custody of the mail in order to pass it on to
                the addressee.  EXAMPLE: c/o John Smith; c/o Best
                Western Hotel</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="countryCodeType">
        <xs:annotation>
            <xs:documentation>A code used to represent a country
                following the ISO 3166 (Alpha3) standard.  EXAMPLES:
                CAN, USA.  RefPDS- COUNTRY_OVERSEAS</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="emailAddressTextType">
        <xs:annotation>
            <xs:documentation>Contains the email address or the website URL.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="externalAddressIdType">
        <xs:annotation>
            <xs:documentation>Identifier for an address that is unique
                in the source system.  Must be used in conjunction with
                Address Source System identifier to be unique.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="externalAddressSourceIdType">
        <xs:annotation>
            <xs:documentation>Source System Identifier for the external address</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:long"/>
    </xs:simpleType>
    <xs:simpleType name="externalServiceAddressIdType">
        <xs:annotation>
            <xs:documentation>Source System Identifier for the external
                service address (e.g. FMS address id)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="fleetMailOfficeNameType">
        <xs:annotation>
            <xs:documentation>The city in which the fleet mail office
                for the ship in question resides (currently either
                Halifax or Victoria) EXAMPLES: Halifax</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="hmcsNameType">
        <xs:annotation>
            <xs:documentation>The name of the relevant Canadian forces
                ship  to be used within a military address. EXAMPLES: Chicoutimi</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="mailingTypeCodeType">
        <xs:annotation>
            <xs:documentation>A code to represent mailability. EXAMPLES:
                M, E  RefPDS- MAILG_TYP</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="municipalityNameType">
        <xs:annotation>
            <xs:documentation>The official municipality name, as
                provided to Canada Post.  Should not be translated.
                (E.G.: TROIS-RIVI¿ES is not THREE RIVERS.) EXAMPLES: Burnaby</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="postOfficeBoxNumberType">
        <xs:annotation>
            <xs:documentation>The assigned number of the post office
                box. The # symbol must not be used, nor the French
                equivalent no. Punctuation should not be used.
                EXAMPLES: 97</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="postalZipCodeType">
        <xs:annotation>
            <xs:documentation>The code assigned by the Canadian or US
                Postal Service to the postal district. For Canadian
                Postal Codes may be 6 characters. For US Zip codes  - It
                may be either five or nine digits. If the nine-digit
                format is used, a hyphen will be used to separate the
                fifth and sixth digits. (This hyphen is not stored in
                the database) EXAMPLES: V2H 7T4</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="provinceStateCodeType">
        <xs:annotation>
            <xs:documentation>The accepted ISO 3166-2, two character
                code of a state, territory or possession of Canada or
                the USA. RefPDS- PROVINCE_STATE</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="relateAddressAssignmentIdType">
        <xs:restriction base="xs:long"/>
    </xs:simpleType>
    <xs:simpleType name="streetDirectionCodeType">
        <xs:annotation>
            <xs:documentation>The code used to represent one of the
                eight principle compass directions.  EXAMPLES: E- East,
                N- North, NE- Northeast, NW- Northwest (English), NO-
                Northwest (French), S- South, SE- Southeast, SW-
                Southwest (English), SO- Southwest (French), W- West
                (English), O- West (French).  RefPDS- STREET_DIR_CA_POST</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="streetNameType">
        <xs:annotation>
            <xs:documentation>The official name recognized by each
                municipality. This name should not be translated. (E.G.:
                &quot;Main&quot; is not translatable to &quot;Principale&quot;.)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="streetTypeCodeType">
        <xs:annotation>
            <xs:documentation>A code to represent a Street Type.
                EXAMPLES: ST, RD, BLVD, CRES.  RefPDS- STREET_TYP_CA_POST</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="unitNumberType">
        <xs:annotation>
            <xs:documentation>Typically a suite or apartment within the
                main building identified by the rest of the address.
                EXAMPLES: 201</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="unitTypeCodeType">
        <xs:annotation>
            <xs:documentation>The type of unit number as defined by
                canada post.  EXAMPLES: Unit, Suite</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="unitTypeDwellingType">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="validateAddressIndicatorType">
        <xs:annotation>
            <xs:documentation>Indicates whether the address should
                undergo validation EXAMPLES: (&quot;Y&quot;) or not (&quot;N&quot;)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="firstNameType">
        <xs:annotation>
            <xs:documentation>First/Given name of an individual</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="fullNameType">
        <xs:annotation>
            <xs:documentation>This is the full name of an individual or
                organization.  It is made up of  a base and an optional
                supplementary components. The base name is either a
                &quot;person name&quot; or an &quot;organization
                name&quot;.  The supplementary portion is made up of a
                &quot;supplementary name formatting keyword code&quot;
                and &quot;supplementary name&quot;.  A &quot;person
                name&quot; is made up of first name, middle name and
                last name.  EXAMPLES: Residential: MARTIN WIENS,
                Residential: DENNIS K Y TONG, Residential: SUSANA
                LEO-YIM, Business: MALCA TRADING INC O/A BANANA LEAF ON
                DENMAN, Business: DIANA BECK D/B FAIRYLAND GIFTS,
                Business: GREATER VAN RENT CHEK SRVCS INC D/B 738
                DIRECTORY, Business: INTERNATIONAL FLIGHT CENTRE INC,
                Business: DR Y VICTOR YOSHIDA</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="lastNameType">
        <xs:annotation>
            <xs:documentation>Last/Family name of an individual</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="middleNameType">
        <xs:annotation>
            <xs:documentation>Middle name of an individual</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="nameAssignmentSubTypeCodeType">
        <xs:annotation>
            <xs:documentation>C- Cleansed, P- Parsed, O- Original</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="nameAssignmentTypeCodeType">
        <xs:annotation>
            <xs:documentation>L- Legal, A- Alias, P- Preferred, I-
                Additional Info</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="nameRelTextType">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="nameSearchTxtType">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="nameSuffixCodeType">
        <xs:annotation>
            <xs:documentation>A code representing the suffix of a name.
                EXAMPLES: Jr for Junior, Esq for Esquire, Sr for Senior.
                RefPDS: NAME_SUFFIX</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="organizationNameType">
        <xs:annotation>
            <xs:documentation>Organization Name</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="relateNameAssignmentIdType">
        <xs:restriction base="xs:long"/>
    </xs:simpleType>
    <xs:simpleType name="salutationCodeType">
        <xs:annotation>
            <xs:documentation>A code representing a salutation which is
                a formal appellation optionally attached to the name of
                a person or family by virtue of office, rank, hereditary
                privilege, noble birth, or attainment or used as a mark
                of respect.  EXAMPLES: Dr. Miss Mr. Mrs. Ms. Colonel
                RefPDS- BI_CM_SALUTATION_CODE</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="supplementaryNameType">
        <xs:annotation>
            <xs:documentation>The extended customer name such as trade
                name for the sole proprietorship business customer. It
                is the trade/firm name that followed the operating type
                such as Operating As (O/A), Doing Business as (D/B).
                EXAMPLES: The full name &quot;MALCA TRADING INC O/A
                BANANA LEAF ON DENMAN&quot; is composed of an
                organization name &quot;MALCA TRADING INC&quot;,
                supplementary name formatting key word of
                &quot;O/A&quot; and supplementary name of &quot;BANANA
                LEAF ON DENMAN&quot;</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="supplementaryNameOperatingCodeType">
        <xs:annotation>
            <xs:documentation>A code representing the formatting key
                word for the supplementary name. EXAMPLES: O/A for
                Operating As D/B for Doing Business as  COMMENTS: The
                full name &quot;MALCA TRADING INC O/A  BANANA LEAF ON
                DENMAN&quot; is composed of an organization name
                &quot;MALCA TRADING INC&quot;, supplementary name
                formatting key word of &quot;O/A&quot; and supplementary
                name of &quot;BANANA LEAF ON DENMAN&quot;.   RefPDS- BI_CM_SUPPLEMENTARY_NAME_OPERATING_CODE</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="customerMarketSegmentId">
        <xs:annotation>
            <xs:documentation>An automatically generated unique
                identifier for customer market segment</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:long"/>
    </xs:simpleType>
    <xs:simpleType name="customerMarketSegmentCodeType">
        <xs:annotation>
            <xs:documentation>The market segment code associated to the
                Customer for the specified market segment type.  RefPDS- MKT_SEG_A</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="customerMarketSegmentTypeCodeType">
        <xs:annotation>
            <xs:documentation>A code indicating the type of scheme of
                categorization used for Customer Market Segmentation.
                EXAMPLES: CBU- CBU Code, SS1- CBU segment A, SS2- CBU
                segment B, VALS- Customer Value segment.  RefPDS- MKT_SEG_TYPE</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="billingAccountNumberCheckDigitNumberType">
        <xs:annotation>
            <xs:documentation>Derived based on the billing account number.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="billingAccountOpenDateType">
        <xs:annotation>
            <xs:documentation>Creation date of the Billing Account.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="billingAccountStatusCodeType">
        <xs:annotation>
            <xs:documentation>A code indicating the status of the
                account. When a new billing account is created, the
                status is Tentative. When charges are distributed to the
                billing account, the status is Open. When no effective
                charges are distributed to the billing account, the
                status is Cancelled. EXAMPLES: O- Open, C- Cancelled.
                RefODS- BI_PAY_BILLING_ACCOUNT_STATUS</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="billingAccountStatusDateType">
        <xs:annotation>
            <xs:documentation>date of the latest billing account status change</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="defaultBillCycleCodeType">
        <xs:annotation>
            <xs:documentation>The default bill-cycle associated with the
                customer.  New billing accounts should use this cycle
                code unless another is requested.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="defaultNewBillCycleCodeType">
        <xs:annotation>
            <xs:documentation>The default new bill-cycle associated with
                the customer.  This is set during a change of bill cycle
                that is the new default.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="billCycleCodeType">
        <xs:annotation>
            <xs:documentation>A code representing the date of billing
                for a billing account</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="newBillCycleCodeType">
        <xs:annotation>
            <xs:documentation>A code representing the date of billing
                for a billing account.  Used for changes in billing
                cycle date</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="billCycleFrequencyCodeType">
        <xs:annotation>
            <xs:documentation>The bill cycle frequency. EXAMPLES: W -
                Week, M - Month.  RefODS- BILL_CYCLE_FREQUENCY_CODE</xs:documentation>
