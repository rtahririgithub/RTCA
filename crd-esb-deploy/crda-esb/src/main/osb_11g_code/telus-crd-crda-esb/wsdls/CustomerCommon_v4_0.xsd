        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="consentValueCodeType">
        <xs:annotation>
            <xs:documentation>The boolean type in XML accepts
                0,1,true,false.  1=Checked, 0=Unchecked.   </xs:documentation>
