        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="autoTreatmentSuppressionEndDateType">
        <xs:annotation>
            <xs:documentation>The date when suppression of autotreatment ends.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="billingAccountExtensionActiveIndicatorType">
        <xs:annotation>
