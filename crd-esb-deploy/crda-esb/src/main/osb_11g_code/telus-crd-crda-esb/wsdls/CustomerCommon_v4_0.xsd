        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="preferredLanguageCodeType">
        <xs:annotation>
            <xs:documentation>e.g. FR, EN</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="preferredContactTimePeriodCodeType">
        <xs:annotation>
            <xs:documentation>e.g. MORNING, NOON, AFTERNOON, EVENING, DAY</xs:documentation>
