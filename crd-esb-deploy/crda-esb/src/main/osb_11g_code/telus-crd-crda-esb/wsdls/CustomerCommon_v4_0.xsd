    <xs:simpleType name="customerCommentTextType">
        <xs:annotation>
            <xs:documentation>Free text comments about the customer</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="customerTypeCodeType">
        <xs:annotation>
            <xs:documentation>B- Business, R- Residential</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="customerSubtypeCodeType">
        <xs:annotation>
            <xs:documentation>I- Individual, O- Organization, U- Unknown</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="creditValueCodeType">
        <xs:annotation>
            <xs:documentation>N- No Credit Information, R- Restricted,
                D- Deposit, U- Unestablished, E- Established, V- VIP
                (TELUS exec).  Possible values in refPDS: CREDIT_VALUE_CODE</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="customerPINType">
        <xs:annotation>
            <xs:documentation>a code the customer uses to authenticate
