    </xs:simpleType>
    <xs:simpleType name="contactVerifiedTsType">
        <xs:annotation>
            <xs:documentation>date that the Contact information,
                including contact role for a Service Address and/or
                RCID, has been verified.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:date"/>
    </xs:simpleType>
    <xs:simpleType name="customerIndividualIndicatorType">
        <xs:annotation>
            <xs:documentation>Indicates that this individual playing the
                role of a contact also plays the role of a customer</xs:documentation>
        </xs:annotation>
