        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="genderCodeType">
        <xs:annotation>
            <xs:documentation>M-  Male, F- Female</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
