    </xs:simpleType>
    <xs:simpleType name="supplementaryNameOperatingCodeType">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <!-- Complex Common Types-->
    <!-- Customer Market Segment Id -->
    <xs:simpleType name="customerMarketSegmentId">
        <xs:restriction base="xs:long"/>
    </xs:simpleType>
    <!-- Customer Market Segment -->
    <xs:complexType name="CustomerMarketSegment">
        <xs:sequence>
            <xs:element minOccurs="0" name="customerMarketSegmentId" type="tns:customerMarketSegmentId"/>
            <xs:element minOccurs="0" name="customerId" type="tns:customerIdType"/>
            <xs:element minOccurs="0" name="marketSegmentCode" type="xs:string"/>
            <xs:element minOccurs="0" name="marketSegmentTypeCode" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <!-- Address types -->
    <xs:complexType name="CivicAddress">
        <xs:sequence>
            <xs:element minOccurs="0" name="civicNumber" type="tns:civicNumberType"/>
            <xs:element minOccurs="0" name="civicNumberSuffix" type="tns:civicNumberSuffixType"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="RuralAddress">
        <xs:sequence>
            <xs:element minOccurs="0" name="ruralRouteNumber" type="tns:ruralRouteNumberType"/>
            <xs:element minOccurs="0" name="ruralRouteTypeCode" type="tns:ruralRouteTypeCodeType"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="StationSquareAddress">
        <xs:sequence>
            <xs:element minOccurs="0" name="stationName" type="tns:stationNameType"/>
            <xs:element minOccurs="0" name="stationQualifier" type="tns:stationQualifierType"/>
            <xs:element minOccurs="0" name="stationTypeCode" type="tns:stationTypeCodeType"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="MilitaryAddress">
        <xs:sequence>
            <xs:element minOccurs="0" name="fleetMailOfficeName" type="tns:fleetMailOfficeNameType"/>
            <xs:element minOccurs="0" name="hmcsName" type="tns:hmcsNameType"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Address">
        <xs:sequence>
            <xs:element minOccurs="0" name="addressId" type="tns:addressIdType"/>
            <xs:element minOccurs="0" name="addressTypeCode" type="tns:addressTypeCodeType"/>
            <xs:element maxOccurs="10"
                name="additionalAddressInformation" type="tns:additionalAddressInformationType"/>
            <xs:element maxOccurs="10" name="renderedAddress" type="tns:renderedAddressType"/>
            <xs:element minOccurs="0" name="addrAssgnmtId" type="tns:addrAssgnmtIdType"/>
            <xs:element minOccurs="0"
                name="addressAssignmentSubTypeCode" type="tns:addressAssignmentSubTypeCodeType"/>
            <xs:element minOccurs="0" name="addressAssignmentTypeCode" type="tns:addressAssignmentTypeCodeType"/>
            <xs:element minOccurs="0" name="addressMatchingStatusCode" type="tns:addressMatchingStatusCodeType"/>
            <xs:element minOccurs="0" name="addressSearchText" type="tns:addressSearchTextType"/>
            <xs:element minOccurs="0" name="canadaPostBuildName" type="tns:canadaPostBuildNameType"/>
            <xs:element minOccurs="0" name="canadaPostLocnName" type="tns:canadaPostLocnNameType"/>
            <xs:element minOccurs="0" name="canadaPostRecordType" type="tns:canadaPostRecordTypeType"/>
            <xs:element minOccurs="0" name="careOf" type="tns:careOfType"/>
            <xs:element minOccurs="0" name="countryCode" type="tns:countryCodeType"/>
            <xs:element minOccurs="0" name="emailAddressText" type="tns:emailAddressTextType"/>
            <xs:element minOccurs="0" name="externalAddressId" type="tns:externalAddressIdType"/>
            <xs:element minOccurs="0" name="externalAddressSourceId" type="tns:externalAddressSourceIdType"/>
            <xs:element minOccurs="0" name="externalServiceAddressId" type="tns:externalServiceAddressIdType"/>
            <xs:element minOccurs="0" name="mailingTypeCode" type="tns:mailingTypeCodeType"/>
            <xs:element minOccurs="0" name="municipalityName" type="tns:municipalityNameType"/>
            <xs:element minOccurs="0" name="postOfficeBoxNumber" type="tns:postOfficeBoxNumberType"/>
            <xs:element minOccurs="0" name="postalZipCode" type="tns:postalZipCodeType"/>
            <xs:element minOccurs="0" name="provinceStateCode" type="tns:provinceStateCodeType"/>
            <xs:element minOccurs="0" name="relateAddressAssignmentId" type="tns:relateAddressAssignmentIdType"/>
            <xs:element minOccurs="0" name="streetDirectionCode" type="tns:streetDirectionCodeType"/>
            <xs:element minOccurs="0" name="streetName" type="tns:streetNameType"/>
            <xs:element minOccurs="0" name="streetTypeCode" type="tns:streetTypeCodeType"/>
            <xs:element minOccurs="0" name="unitNumber" type="tns:unitNumberType"/>
            <xs:element minOccurs="0" name="unitTypeCode" type="tns:unitTypeCodeType"/>
            <xs:element minOccurs="0" name="validateAddressIndicator" type="tns:validateAddressIndicatorType"/>
            <xs:choice>
                <xs:element name="civicAddress" type="tns:CivicAddress"/>
                <xs:element name="ruralAddress" type="tns:RuralAddress"/>
                <xs:element name="stationSquareAddress" type="tns:StationSquareAddress"/>
                <xs:element name="militaryAddress" type="tns:MilitaryAddress"/>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Name">
        <xs:sequence>
            <xs:element minOccurs="0" name="nameId" type="tns:nameIdType"/>
            <xs:element minOccurs="0" name="nameTypeCode" type="tns:nameTypeCodeType"/>
            <xs:element minOccurs="0" name="firstName" type="tns:firstNameType"/>
            <xs:element minOccurs="0" name="fullName" type="tns:fullNameType"/>
            <xs:element minOccurs="0" name="language" type="tns:languageType"/>
            <xs:element minOccurs="0" name="lastName" type="tns:lastNameType"/>
            <xs:element minOccurs="0" name="middleName" type="tns:middleNameType"/>
            <xs:element minOccurs="0" name="nameAssignmentSubTypeCode" type="tns:nameAssignmentSubTypeCodeType"/>
            <xs:element minOccurs="0" name="nameAssignmentTypeCode" type="tns:nameAssignmentTypeCodeType"/>
            <xs:element minOccurs="0" name="nameLine1Text" type="tns:nameLineTextType"/>
            <xs:element minOccurs="0" name="nameLine2Text" type="tns:nameLineTextType"/>
            <xs:element minOccurs="0" name="nameRelText" type="tns:nameRelTextType"/>
            <xs:element minOccurs="0" name="nameSearchTxt" type="tns:nameSearchTxtType"/>
            <xs:element minOccurs="0" name="nameSuffix2Code" type="tns:nameSuffixCodeType"/>
            <xs:element minOccurs="0" name="nameSuffixCode" type="tns:nameSuffixCodeType"/>
            <xs:element minOccurs="0" name="organizationName" type="tns:organizationNameType"/>
            <xs:element minOccurs="0" name="relateNameAssignmentId" type="tns:relateNameAssignmentIdType"/>
            <xs:element minOccurs="0" name="salutationCode" type="tns:salutationCodeType"/>
            <xs:element minOccurs="0" name="supplementaryName" type="tns:supplementaryNameType"/>
            <xs:element minOccurs="0"
                name="supplementaryNameOperatingCode" type="tns:supplementaryNameOperatingCodeType"/>
        </xs:sequence>
    </xs:complexType>
</xs:schema>