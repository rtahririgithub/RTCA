		<nsAbbrev>ODCP</nsAbbrev>
		<OutputDir/>
		<SecurityToken>
			<Token>Wssp1.2-2007-Saml1.1-SenderVouches-Wss1.0</Token>
		</SecurityToken>
	</ServiceDefinition>
	<Operation>
		<OperationName>calculateDeposit</OperationName>
