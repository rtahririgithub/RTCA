			<xs:element name="balanceOwingOnICA" type="tns:moneyType" minOccurs="0"/>
			<xs:element name="latestInvoluntaryCancelledDate" type="xs:date" minOccurs="0"/>
			<xs:element name="noOfNSFCheques" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>No of NSF Cheques in the past 12 months.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- added following 4 attribute in RTCA 1.6 -->
			<xs:element name="baScore" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>BA Score</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="collectionSegment" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Collection Segmenet</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="scoreCardID" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Score card ID</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="delinquencyCycle" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>delinquency cycle</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="CreditAssessmentResult">
		<xs:complexType>
			<xs:annotation>
				<xs:documentation>Credit decision-ing (FICO) assessment
                result </xs:documentation>
			</xs:annotation>
			<xs:sequence>
				<xs:element name="assessmentResultCd" type="xs:string">
					<xs:annotation>
						<xs:documentation>As provided by Decision-ing
                        engine. operation result code . Sucessfull,
                        Partilly ,...</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="assessmentResultReasonCd" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>As provided by Decision-ing
                        engine. operation result code reason.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="decisionCd" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Decision Code from FICO. RTCA1.5</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="assessmentMessageCd" type="xs:string">
					<xs:annotation>
						<xs:documentation>As provided by Decision-ing
                        engine. such deposit required,...</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="creditValueCd" type="tns:creditValueCodeType"/>
				<xs:element name="productCategoryQualification" type="tns:ProductCategoryQualification" minOccurs="0"/>
				<xs:element name="fraudIndicatorCd" type="tns:fraudIndicatorType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>As determined by Decision-ing
                        engine  or overriden by agent.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="fraudMessageCdList" type="tns:fraudMessageType" minOccurs="0" maxOccurs="100">
					<xs:annotation>
						<xs:documentation>As determined by Decision-ing
                        engine rule. </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="bureauInformationList" type="tns:BureauInformation" minOccurs="0" maxOccurs="100"/>
				<xs:element name="rulesExecuted" type="xs:string" minOccurs="0" maxOccurs="100"/>
				<xs:element name="creditAssessmentStrategies" type="tns:CreditAssessmentStrategies" minOccurs="0"/>
				<xs:element name="creditRiskLevel" type="xs:int" minOccurs="0">
					<xs:annotation>
						<xs:documentation>RTCA risk level</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="unifiedCreditAssessmentResult" type="tns:UnifiedCreditAssessmentResult" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="CreditAssessmentStrategies">
		<xs:annotation>
			<xs:documentation>Credit Assessment Strategies, Possible
                values are:CHAM (Champion Strategy),CHAL (Challenger Strategy)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="depositLookupStrategy" type="xs:string" minOccurs="0"/>
			<xs:element name="cvudStrategy" type="xs:string" minOccurs="0"/>
			<xs:element name="creditValueAssignmentStrategy" type="xs:string" minOccurs="0"/>
			<xs:element name="customerActivityStrategy" type="xs:string" minOccurs="0"/>
			<xs:element name="tenureStrategy" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CreditBureauResult">
		<xs:annotation>
			<xs:documentation>Credit Gateway or Credit Decision-ing
                Simulator result</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="xs:long" minOccurs="0"/>
			<xs:element name="personName" type="tns:PersonName" minOccurs="0"/>
			<xs:element name="errorCd" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Error code returned from Vendor.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reportSourceCd" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Vendor name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reportType" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Credit report type: consumer,
                        commerical,corporate registery, etc </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="printImageReportTxt" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Print image report (PI or readable format) representing  simulated credit bureau report text </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="adjudicationResult" type="tns:AdjudicationResult" minOccurs="0"/>
			<xs:element name="riskIndicator" type="tns:RiskIndicator" minOccurs="0"/>
			<xs:element name="fraudWarningList" type="tns:FraudWarning" minOccurs="0" maxOccurs="100"/>
			<xs:element name="creationDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>credit check timestamp</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PersonName">
		<xs:sequence>
			<xs:element name="firstName" type="xs:string"/>
			<xs:element name="lastName" type="xs:string"/>
			<xs:element name="middleName" type="xs:string" minOccurs="0"/>
			<xs:element name="nameSuffix" type="xs:string" minOccurs="0"/>
			<xs:element name="title" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!--RTCA 1.5 -->
	<xs:complexType name="DepositData">
		<xs:annotation>
			<xs:documentation>Deposit Data</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="depositPaid" type="tns:moneyType" minOccurs="0"/>
			<xs:element name="depositPending" type="tns:moneyType" minOccurs="0"/>
			<xs:element name="depositReleased" type="tns:moneyType" minOccurs="0"/>
			<xs:element name="mostRecentDepositPaidDate" type="xs:date" minOccurs="0"/>
			<xs:element name="mostRecentDepositPendingDate" type="xs:date" minOccurs="0"/>
			<xs:element name="mostRecentDepositReleaseDate" type="xs:date" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!--Unified Credit-->
	<!-- *************************************common***********************************************************-->
	<xs:complexType name="IndicatorType">
		<xs:sequence>
			<xs:element name="indicator" type="xs:boolean" minOccurs="0"/>
			<xs:element name="reasonCd" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- *************************************wln***********************************************************-->
	<xs:complexType name="WlnMatchedCustomer">
		<xs:sequence>
			<xs:element name="customerData" type="tns:WlnCustomerInfo"/>
			<xs:element name="accountData" type="tns:WlnAccountInfo"/>
			<xs:element name="creditWorthinessData" type="tns:WlnCreditWorthinessData"/>
			<xs:element name="collectionData" minOccurs="0">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="tns:WlnCreditCollectionData">
							<xs:sequence>
								<xs:element name="collectionInd" type="xs:boolean" minOccurs="0"/>
								<!-- added following 4 attribute in RTCA 1.6 -->
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="creditBureauResult" type="tns:WlnCreditBureauResult" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WlnCustomerInfo">
		<xs:sequence>
			<xs:element name="customerId" type="xs:long"/>
			<xs:element name="customerCreationDate" type="xs:date"/>
			<xs:element name="customerStatusCd" type="xs:string"/>
			<xs:element name="customerTypeCd" type="xs:string"/>
			<xs:element name="customerSubTypeCd" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WlnCreditWorthinessData">
		<xs:annotation>
			<xs:documentation>Credit Worthiness Input Data</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="creditValueCd" type="xs:string" minOccurs="0"/>
			<xs:element name="creditRiskLevel" type="xs:int" minOccurs="0"/>
			<xs:element name="decisionCd" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>represent the rules that led to this assessment result.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fraudIndicatorCd" type="xs:string" minOccurs="0"/>
			<xs:element name="fraudMessageCdList" type="xs:string" minOccurs="0" maxOccurs="100"/>
			<!--Added in PTCA 1.6-->
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WlnCreditProfileData">
		<xs:sequence/>
	</xs:complexType>
	<xs:complexType name="WlnCreditBureauResult">
		<xs:annotation>
			<xs:documentation>Credit Gateway or Credit Decision-ing
 Simulator result</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="reportSourceCd" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Vendor name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="adjudicationResult" type="tns:WlnAdjudicationResult" minOccurs="0"/>
			<xs:element name="errorCd" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Error code returned from Vendor.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="creationDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>credit check timestamp</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
