		<xs:sequence>
			<xs:element name="unifiedCreditAssessmentIndicator" type="tns:IndicatorType">
				<xs:annotation>
					<xs:documentation>Indicates  whether the assessment result is based on Unified Credit Rules or not . And provides reason code(s) to indicate why the top match customer/account cannot be utilized</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="unifiedCreditSimulatedBureauResultData" type="tns:CreditBureauResult" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
