			<xs:extension base="tns:BaseUnifiedCreditSearchResult">
				<xs:sequence>
					<xs:element name="matchedCustomer" type="tns:WlnMatchedCustomer" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="WlsUnifiedCreditSearchResult">
		<xs:complexContent>
			<xs:extension base="tns:BaseUnifiedCreditSearchResult">
				<xs:sequence>
					<xs:element name="matchedAccount" type="tns:WlsMatchedAccount" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
