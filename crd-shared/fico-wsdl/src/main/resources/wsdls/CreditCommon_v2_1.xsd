		<xs:sequence>
			<xs:element name="creditDecision" type="tns:CreditDecision" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WlnCreditCollectionData">
		<xs:annotation>
			<xs:documentation>Collection Data</xs:documentation>
		</xs:annotation>
	</xs:complexType>
	<xs:complexType name="WlnAccountInfo">
		<xs:annotation>
			<xs:documentation>Rrepresent an account in CODS. The account could be a wln or a wls account residing in CODS.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="billingAccountNumber" type="xs:long"/>
			<xs:element name="billingMasterSourceId" type="xs:long"/>
			<xs:element name="brandId" type="xs:long" minOccurs="0"/>
			<xs:element name="openDate" type="xs:date" minOccurs="0"/>
			<xs:element name="statusCode" type="xs:string" minOccurs="0"/>
			<xs:element name="statusDate" type="xs:date" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<!-- *************************************wls***********************************************************-->
	<xs:complexType name="WlsMatchedAccount">
		<xs:sequence>
			<xs:element name="accountData" type="tns:WlsAccountInfo"/>
			<xs:element name="creditWorthinessData" type="tns:WlsCreditWorthinessData"/>
			<xs:element name="warningHistoryList" type="tns:WlsWarningHistory" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Represent the list of new and past warning associated to an account</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="accountFinancialHistory" type="tns:WlsAccountFinancialHistory"/>
			<xs:element name="creditBureauResult" type="tns:WlsCreditBureauResult" minOccurs="0"/>
			<xs:element name="totalSubscribers">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="totalActiveSubscribersNumber" type="xs:int"/>
						<xs:element name="totalReservedSubscribersNumber" type="xs:int"/>
						<xs:element name="totalSuspendedSubscribersNumber" type="xs:int"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WlsAccountInfo">
		<xs:sequence>
			<xs:element name="billingAccountNumber" type="xs:long"/>
			<xs:element name="accountType" type="xs:string"/>
			<xs:element name="accountSubType" type="xs:string"/>
			<xs:element name="brandId" type="xs:string"/>
			<xs:element name="accountCreationDate" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Same as startServiceDate if startServiceDate is not empty otherwise same as accountStatusDate </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="startServiceDate" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Represents the date when account became open (status O) – which is the date when the first subscriber was activated. This date does not change if the subscriber is canceled or more subs are added to the account.    </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="accountStatus" type="xs:string"/>
			<xs:element name="accountStatusDate" type="xs:dateTime"/>
			<xs:element name="statusActivityCode" type="xs:string" minOccurs="0"/>
			<xs:element name="statusActivityReasonCode" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WlsWarningHistoryBase">
		<xs:sequence>
			<xs:element name="warningCategoryCd" type="xs:string">
				<xs:annotation>
					<xs:documentation>Represent the category of warnings ( internal value) : Iany warning stored in fraud db , Warning from Bureua , .... . </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="warningCd" type="xs:string">
				<xs:annotation>
					<xs:documentation>Unique Code representing a warning.  for example  warningCd=Frepresent a Written off Blacklisted ID  exist, DNA (Do Not Activate) , or warningCD=ID_FRAUD_01 represent a Identifiy Fraud </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="warningTypeCd" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Represent the type of warning such as SAFESCAN,HAWK,..</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="warningStatusCd" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>populated by credit decisioning, stored in cda DB </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WlsWarningHistory">
		<xs:complexContent>
			<xs:extension base="tns:WlsWarningHistoryBase"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="WlsCreditWorthinessData">
		<xs:sequence>
			<xs:element name="riskLevelNumber" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A numeric value representing the risk level assessed for an account .( the higher the number the higher the risk)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="riskLevelDecisionCd" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>It uniquely identifies a decision made which determines the Risk Level, for reporting and analytic purpose only.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="creditClassCd" type="xs:string">
				<xs:annotation>
					<xs:documentation>represents the credit class . It is stored in account's creditprofile(CDA and KB Databases)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WlsAccountFinancialHistory">
		<xs:sequence>
			<xs:element name="delinquentInd" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation> </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WlsCreditBureauResult">
		<xs:annotation>
			<xs:documentation>Credit Gateway or Credit Decision-ing
 Simulator result</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="reportSourceCd" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Vendor name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="errorCd" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Error code returned from Vendor.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="adjudicationResult" type="tns:WlnAdjudicationResult" minOccurs="0"/>
			<xs:element name="creationDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>credit check timestamp</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- *************************************Unified Credit input***********************************************************-->
	<xs:complexType name="BaseUnifiedCreditSearchResult" abstract="true">
		<xs:sequence>
			<xs:element name="matchFound" type="tns:IndicatorType"/>
			<xs:element name="dataInquiryErrorCodeList" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="lineOfBusiness" type="xs:string" minOccurs="0"/>
			<xs:element name="unifiedCreditDormantInd" type="xs:boolean"/>
		</xs:sequence>
